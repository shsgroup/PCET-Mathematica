Cell["Channel contributions vs. reaction free energy", "Subsubsection",
 CellChangeTimes->{{3.43244508556349*^9, 3.4324450940391417`*^9}, 
   3.432575045290399*^9, {3.432610316376317*^9, 3.432610322863893*^9}}],

Cell["\<\
Parameter description:
Grange={\[CapitalDelta]1,\[CapitalDelta]2,\[Delta]} - (list) range of \
\[CapitalDelta]G: from \[CapitalDelta]1 to \[CapitalDelta]2 with stepsize \
\[Delta] (i.e. (1+(\[CapitalDelta]2-\[CapitalDelta]1)/\[Delta]) points will \
be plotted);
n, m - highest quantum number for reactant and product states required for \
convergence in the whole range of \[CapitalDelta]G's;
nout, mout - highest quantum number of reactant and product states for which \
the contributions will be plotted.\
\>", "Text",
 CellChangeTimes->{{3.4326642813606243`*^9, 3.43266438346307*^9}, {
  3.4326644161681337`*^9, 3.432664586778017*^9}, {3.432666902015031*^9, 
  3.432666956351789*^9}, {3.4326671989481697`*^9, 3.432667226260045*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"qfluxcontributions", "[", 
    RowBox[{
    "T_", ",", "Grange_", ",", "\[Lambda]_", ",", "R_", ",", "\[Mu]_", ",", 
     "\[Omega]_", ",", "n_", ",", "m_", ",", "nout_", ",", "mout_"}], "]"}], ":=",
    "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "dG", ",", "\[CapitalDelta]1", ",", " ", "\[CapitalDelta]2", ",", " ", 
       "\[Delta]", ",", "Hmatrixlist", ",", "Dmatrixlist", ",", "i", ",", 
       "j"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "\[CapitalDelta]1", ",", "\[CapitalDelta]2", ",", "\[Delta]"}], "}"}],
        "=", "Grange"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Monitor", "[", 
       RowBox[{
        RowBox[{"Hmatrixlist", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"rateqfluxHmatrix", "[", 
            RowBox[{
            "T", ",", "dG", ",", "\[Lambda]", ",", "R", ",", "\[Mu]", ",", 
             "\[Omega]", ",", "n", ",", "m"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{
            "dG", ",", "\[CapitalDelta]1", ",", "\[CapitalDelta]2", ",", 
             "\[Delta]"}], "}"}]}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"\"\<Hydrogen: \[CapitalDelta]G\>\"", "\[Rule]", "dG"}], 
          ",", 
          RowBox[{"ProgressIndicator", "[", 
           RowBox[{
            RowBox[{"Dynamic", "[", "dG", "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"\[CapitalDelta]1", ",", "\[CapitalDelta]2"}], "}"}]}], 
           "]"}]}], "}"}]}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Monitor", "[", 
       RowBox[{
        RowBox[{"Dmatrixlist", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"rateqfluxDmatrix", "[", 
            RowBox[{
            "T", ",", "dG", ",", "\[Lambda]", ",", "R", ",", "\[Mu]", ",", 
             "\[Omega]", ",", "n", ",", "m"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{
            "dG", ",", "\[CapitalDelta]1", ",", "\[CapitalDelta]2", ",", 
             "\[Delta]"}], "}"}]}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"\"\<Deuterium: \[CapitalDelta]G\>\"", "\[Rule]", "dG"}], 
