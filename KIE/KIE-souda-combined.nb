           "]"}]}], "}"}]}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"ListLinePlot", "[", 
       RowBox[{
        RowBox[{"Join", "[", 
         RowBox[{
          RowBox[{"Flatten", "[", 
           RowBox[{
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"Tooltip", "[", 
               RowBox[{
                RowBox[{"Hmatrixlist", "\[LeftDoubleBracket]", 
                 RowBox[{"All", ",", "i", ",", "j"}], 
                 "\[RightDoubleBracket]"}], ",", 
                RowBox[{
