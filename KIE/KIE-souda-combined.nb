                  RowBox[{"j", "-", "1"}], "]"}], "<>", "\"\<(H)\>\""}]}], 
               "]"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", "1", ",", 
                RowBox[{"nout", "+", "1"}]}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", "1", ",", 
                RowBox[{"mout", "+", "1"}]}], "}"}]}], "]"}], ",", "1"}], 
           "]"}], ",", 
          RowBox[{"Flatten", "[", 
           RowBox[{
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"Tooltip", "[", 
               RowBox[{
                RowBox[{"Dmatrixlist", "\[LeftDoubleBracket]", 
                 RowBox[{"All", ",", "i", ",", "j"}], 
                 "\[RightDoubleBracket]"}], ",", 
                RowBox[{
                 RowBox[{"ToString", "[", 
                  RowBox[{"i", "-", "1"}], "]"}], "<>", "\"\<->\>\"", "<>", 
